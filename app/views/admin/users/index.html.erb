<table class='table'>
  <tr>
    <th>Name</th>

    <th>Provider</th>

    <th>Admin</th>

    <th>Registration date</th>
  </tr>
  <% @users.each do |user| %>
    <tr>
      <td>
        <%= link_to user.name, edit_admin_user_path(user) %>
      </td>

      <td><%= user.provider %></td>

      <td><%= user.admin %></td>

      <td><%= user.created_at %></td>

    </tr>
  <% end %>
</table>
